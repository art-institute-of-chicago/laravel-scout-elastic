{
    "name": "aic/laravel-scout-elastic",
    "description": "Elastic Driver for Laravel Scout",
    "authors": [
        {
            "name": "Erick Tamayo",
            "email": "ErickTamayo@users.noreply.github.com"
        }
    ],
    "keywords": ["laravel", "scout", "elasticsearch", "elastic"],
    "require": {
        "php": "^8.1",
        "laravel/scout": "^10.0"
    },
     "require-dev": {
        "elasticsearch/elasticsearch": "^7.9",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^7.15.0)."
    },
    "autoload": {
        "psr-4": {
            "Tamayo\\LaravelScoutElastic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tamayo\\LaravelScoutElastic\\LaravelScoutElasticProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
